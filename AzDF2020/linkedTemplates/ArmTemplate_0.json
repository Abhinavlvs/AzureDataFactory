{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzDF2020"
		},
		"LS_AzDataLakeStr_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@linkedService().BlobUrl"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/LS_AzDataLakeStr')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureBlobFS",
				"annotations": [],
				"parameters": {
					"BlobUrl": {
						"type": "string",
						"defaultValue": "https://azdatalakeg2.dfs.core.windows.net"
					}
				},
				"typeProperties": {
					"url": "[parameters('LS_AzDataLakeStr_properties_typeProperties_url')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/DS_AzDataLakeStr')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LS_AzDataLakeStr",
					"type": "LinkedServiceReference",
					"parameters": {
						"BlobUrl": {
							"value": "@dataset().DataLakeURI",
							"type": "Expression"
						}
					}
				},
				"parameters": {
					"DataLakeURI": {
						"type": "string",
						"defaultValue": "https://azdatalakeg2.dfs.core.windows.net"
					},
					"Contianer": {
						"type": "string",
						"defaultValue": "dlcont01"
					},
					"FilePath": {
						"type": "string",
						"defaultValue": "DimReseller.csv"
					}
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@dataset().FilePath",
							"type": "Expression"
						},
						"fileSystem": {
							"value": "@dataset().Contianer",
							"type": "Expression"
						}
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS_AzDataLakeStr')]"
			]
		}
	]
}
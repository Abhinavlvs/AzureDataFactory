{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzDFJUN20"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/OnPremSQLSrvr_DS')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "OnPremSQLSrvr_LS",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"OnPremSQLSrvr_Table_Schema": {
						"type": "String",
						"defaultValue": "dbo"
					},
					"OnPremSQLSrvr_Table_Name": {
						"type": "String",
						"defaultValue": "students"
					}
				},
				"annotations": [],
				"type": "SqlServerTable",
				"schema": [
					{
						"name": "student_id",
						"type": "int",
						"precision": 10
					},
					{
						"name": "student_name",
						"type": "varchar"
					}
				],
				"typeProperties": {
					"schema": {
						"value": "@dataset().OnPremSQLSrvr_Table_Schema",
						"type": "Expression"
					},
					"table": {
						"value": "@dataset().OnPremSQLSrvr_Table_Name",
						"type": "Expression"
					}
				}
			},
			"dependsOn": []
		}
	]
}
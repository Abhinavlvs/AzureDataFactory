{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzDF20"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LS_AzureDataLakeG2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AzureDataLakeG2'"
		},
		"LS_OnPremSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_OnPremSQLServer'"
		},
		"LS_OracleDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_OracleDB'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdlstr.dfs.core.windows.net"
		},
		"HTTP_SOF_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.stackexchange.com/2.2/posts?order=desc&sort=activity&site=stackoverflow"
		},
		"LS_AzureDataLakeG2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azdlstr.dfs.core.windows.net"
		},
		"LS_HttpServer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.stackexchange.com/2.2/posts?order=desc&sort=activity&site=stackoverflow"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.stackexchange.com/2.2/posts?order=desc&sort=activity&site=stackoverflow"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LS_AzureDataLakeG2_accountKey": {
						"value": "[parameters('LS_AzureDataLakeG2_accountKey')]"
					},
					"LS_OnPremSQLServer_connectionString": {
						"value": "[parameters('LS_OnPremSQLServer_connectionString')]"
					},
					"LS_OracleDB_connectionString": {
						"value": "[parameters('LS_OracleDB_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"HTTP_SOF_properties_typeProperties_url": {
						"value": "[parameters('HTTP_SOF_properties_typeProperties_url')]"
					},
					"LS_AzureDataLakeG2_properties_typeProperties_url": {
						"value": "[parameters('LS_AzureDataLakeG2_properties_typeProperties_url')]"
					},
					"LS_HttpServer_properties_typeProperties_url": {
						"value": "[parameters('LS_HttpServer_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}